@model IndexViewModel
@{
  ViewData["Title"] = "Profile";
}

<div class="row">
  <div class="col-md-6">
    <form id="profile-form" method="post">
      <div class="form-group">
        <label asp-for="FirstName"></label>
        <input asp-for="FirstName" class="form-control" disabled/>
      </div>
      <div class="form-group">
        <label asp-for="LastName"></label>
        <input asp-for="LastName" class="form-control" disabled/>
      </div>
      <div class="form-group">
        <label asp-for="OfficeLocation"></label>
        <input asp-for="OfficeLocation" class="form-control" disabled/>
      </div>
      <div class="form-group">
        <label asp-for="EmailAddress"></label>
        <input asp-for="EmailAddress" class="form-control" disabled/>
      </div>
      <div class="form-group">
        <label asp-for="StartDate"></label>
        <input asp-for="StartDate" class="form-control" disabled/>
      </div>
      <button type="submit" class="btn btn-primary">Save</button>
    </form>
  </div>
</div>

@section Scripts {
  <partial name="_ValidationScriptsPartial" />
}